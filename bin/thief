#!/usr/bin/env ruby
require 'rubygems' if RUBY_VERSION < '1.9'
require 'bundler'
Bundler.setup

$:.push File.expand_path('../../lib', __FILE__)
require 'thief'

Thief.setup!

vorname  = "Angela"
nachname = "Merkel"

Thief.fetch(:first_name => vorname, :last_name => nachname)
Thief.integrate!

Thief::Person.all.each { |person| puts "#{person.name}" }

